{
  "name": "dt-toolbox",
  "description": "Data manipulation tool",
  "version": "7.4.4",
  "license": "MIT",
  "author": "Peter Naydenov",
  "main": "./dist/dtbox.umd.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/dtbox.esm.mjs",
      "require": "./dist/dtbox.cjs",
      "default": "./dist/dtbox.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./src/*": "./src/*"
  },
  "scripts": {
    "start": "node src/main.js",
    "test": "mocha test",
    "cover": "c8 mocha test",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeterNaydenov/dt-toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/PeterNaydenov/dt-toolbox/issues"
  },
  "homepage": "https://github.com/PeterNaydenov/dt-toolbox#readme",
  "dependencies": {
    "@peter.naydenov/walk": "5.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "c8": "^10.1.3",
    "chai": "^5.2.1",
    "mocha": "^11.7.1",
    "rollup": "^4.46.2"
  },
  "c8": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules",
      "test",
      "test-data"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "keywords": [
    "flatten",
    "shallow",
    "data",
    "object",
    "structure",
    "dt"
  ]
}
